{{guardarValor false}}
<div class="container p-4">
    <div class="row">
        <div class="col-md-12 text-center">
            <h3 style="color: white;">{{titulo}}</h3>
        </div>
    </div>
</div>

<div class="container">
    <table class="table table-striped table-dark mx-auto">
        <thead>
            <tr>
                <th scope="col">Numero de compras</th>
                <th scope="col">Producto</th>
                <th scope="col">Categoria</th>
            </tr>
        </thead>
        <tbody>
            {{#each compras}}
            <tr>
                <td>{{numero}}</td>
                <td>{{nombre_producto}}</td>
                <td>{{nombre_cp}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<div class="container p-4">
    <div class="row">
        <div class="col-md-12 text-center">
            <h3 style="color: white;">{{desc}} {{limit.nombre_producto}}</h3>
        </div>
    </div>
</div>